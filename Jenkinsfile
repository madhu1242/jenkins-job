pipeline {
    agent any
    stages {
        stage('EMPLOIYEE NAME') {
            steps {
                echo 'RODDAM MADHURIMA'
            }
        }
        stage('WORK EXP'){
            steps{
                echo '5 YEARS'
            }
        }
        stage('CURRENT COMPANY') {
            steps{
                echo'TCTSL'
            }
        }
    }
    post { 
        always { 
            echo 'Running after the stages'
        }
    }
}